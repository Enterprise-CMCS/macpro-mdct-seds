{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Status",
  "description": "State Status",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "status": {
        "type": "string",
        "description": "Status name for state"
      },
      "status_description": {
        "type": "string",
        "description": "Description of what it means for a form to be in this status"
      },
      "statusId": {
        "type": "string",
        "description": "Status ID"
      }
    },
    "required": ["status", "status_description", "statusId"]
  }
}
