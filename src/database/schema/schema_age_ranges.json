{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Age Ranges",
  "description": "Age range specifications",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "ageRange": {
        "type": "string",
        "description": "StartAge - EndAge"
      },
      "ageDescription": {
        "type": "string",
        "description": "Description of age range"
      },
      "rangeId": {
        "type": "string",
        "description": "Unique ID for each age range"
      }
    },
    "required": ["ageRange", "ageDescription", "rangeId"]
  }
}
